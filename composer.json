{
    "name": "benjacho/belongs-to-many-field",
    "description": "belongsToMany nova representation in field.",
    "keywords": [
        "laravel",
        "nova",
        "belongsToMany",
        "Many to Many Field"
    ],
    "license": "MIT",
  "authors": [
    {
      "name": "Benjamin Flores",
      "email": "christianbfc97@gmail.com",
      "homepage": "https://github.com/Benjacho"
    }
  ],
  "require": {
        "php": "^8.1",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Benjacho\\BelongsToManyField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Benjacho\\BelongsToManyField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    }
}
